<mujocoinclude> 
    <!--##############
        ### IMPORT ###
        ############## -->
    <!-- WAM BHAND -->
    <asset>
        <mesh file="/home/arnab/UWARL_catkin_ws/src/uwarl-mujoco-summit-wam-sim/meshes/meshes_bhand/bhand_palm_fine.stl"/>
        <mesh file="/home/arnab/UWARL_catkin_ws/src/uwarl-mujoco-summit-wam-sim/meshes/meshes_bhand/bhand_palm_link_convex_decomposition_p1.stl"/>
        <mesh file="/home/arnab/UWARL_catkin_ws/src/uwarl-mujoco-summit-wam-sim/meshes/meshes_bhand/bhand_palm_link_convex_decomposition_p2.stl"/>
        <mesh file="/home/arnab/UWARL_catkin_ws/src/uwarl-mujoco-summit-wam-sim/meshes/meshes_bhand/bhand_palm_link_convex_decomposition_p3.stl"/>
        <mesh file="/home/arnab/UWARL_catkin_ws/src/uwarl-mujoco-summit-wam-sim/meshes/meshes_bhand/bhand_palm_link_convex_decomposition_p4.stl"/>
        <mesh file="/home/arnab/UWARL_catkin_ws/src/uwarl-mujoco-summit-wam-sim/meshes/meshes_bhand/bhand_finger_prox_link_fine.stl"/>
        <mesh file="/home/arnab/UWARL_catkin_ws/src/uwarl-mujoco-summit-wam-sim/meshes/meshes_bhand/bhand_finger_prox_link_convex_decomposition_p1.stl"/>
        <mesh file="/home/arnab/UWARL_catkin_ws/src/uwarl-mujoco-summit-wam-sim/meshes/meshes_bhand/bhand_finger_prox_link_convex_decomposition_p2.stl"/>
        <mesh file="/home/arnab/UWARL_catkin_ws/src/uwarl-mujoco-summit-wam-sim/meshes/meshes_bhand/bhand_finger_prox_link_convex_decomposition_p3.stl"/>
        <mesh file="/home/arnab/UWARL_catkin_ws/src/uwarl-mujoco-summit-wam-sim/meshes/meshes_bhand/bhand_finger_med_link_fine.stl"/>
        <mesh file="/home/arnab/UWARL_catkin_ws/src/uwarl-mujoco-summit-wam-sim/meshes/meshes_bhand/bhand_finger_med_link_convex.stl"/>
        <mesh file="/home/arnab/UWARL_catkin_ws/src/uwarl-mujoco-summit-wam-sim/meshes/meshes_bhand/bhand_finger_dist_link_fine.stl"/>
        <mesh file="/home/arnab/UWARL_catkin_ws/src/uwarl-mujoco-summit-wam-sim/meshes/meshes_bhand/bhand_finger_dist_link_convex.stl"/>
    </asset>


    <!--###############
        ### Config: ###
        ############### -->
    <!-- DEFAULT --> 
    <default>
        <default class="wam/bhand">
            <joint type="hinge" limited="true" pos="0 0 0" axis="0 0 1"  frictionloss=".001"/>
        </default>
        <default class="wam/bhand/col">
            <geom type="mesh" contype="1" conaffinity="1" group="0" rgba="0.5 0.6 0.7 0.7"/>          
        </default>
        <default class="wam/bhand/viz">
            <geom type="mesh" contype="0" conaffinity="0" group="1" rgba="0.4 0.4 0.4 1"/>
        </default>
        <default class="wam/bhand/bhand_palm_link/viz">
            <geom type="mesh" contype="0" conaffinity="0" group="1" rgba="0.4 0.4 0.4 1"/>
        </default>
        <default class="wam/bhand/bhand_finger_prox_link/viz">
            <geom type="mesh" contype="0" conaffinity="0" group="1" rgba="0.4 0.4 0.4 1"/>
        </default>
        <default class="wam/bhand/bhand_finger_med_link/viz">
            <geom type="mesh" contype="0" conaffinity="0" group="1" rgba="0.2 0.4 0.65 1"/>
        </default>
        <default class="wam/bhand/bhand_finger_dist_link/viz">
            <geom type="mesh" contype="0" conaffinity="0" group="1" rgba="0.05 0.4 0.8 1"/>
        </default>
    </default>
    
    <!-- EQUALITY -->
    <!-- <equality>
        Joint Constraints: 
            - SPEC:
                > Note that joint 1 in the knuckle drives the outer link first through a 93.75 reduction 
                    and then a 4:1 reduction.
                > In this state, the ratios of motor position to joint position for the 1 stand 2 ndfinger 
                    joints are 93.75:1 and 125:1, respectively.
            - Note:
                - Hence, the motion contraint is based on 93.75:125 * 4:1 = 3:1 reduction, 
                    which is close to the range of motion ratio 140:48 [deg] === 2.9167:1 reduction
       
        <joint joint1="bhand/f1/prox" joint2="bhand/f2/prox" polycoef="0 1 0 0 0"/>
        <joint joint1="bhand/f1/med" joint2="bhand/f1/dist" polycoef="0 3 0 0 0"/>
        <joint joint1="bhand/f2/med" joint2="bhand/f2/dist" polycoef="0 3 0 0 0"/>
        <joint joint1="bhand/f3/med" joint2="bhand/f3/dist" polycoef="0 3 0 0 0"/>
    </equality> -->

    <!-- CONTACT -->
    <contact>
        <exclude body1="wam/bhand/bhand_palm_link" body2="wam/bhand/finger_1/med_link"/>
        <exclude body1="wam/bhand/bhand_palm_link" body2="wam/bhand/finger_2/med_link"/>
    </contact>

</mujocoinclude>