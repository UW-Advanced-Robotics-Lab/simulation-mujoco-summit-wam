                <!-- === Shoulder Lift Link === -->
                <body name="fetch/shoulder_lift_link" pos="0.117 0 0.06" quat="1 0 0 0">
                    <geom name="fetch/viz/shoulder_lift_link"   class="fetch/shoulder_lift_link/viz" mesh="shoulder_lift_link"/>
                    <geom name="fetch/col/shoulder_lift_link"   class="fetch/col" mesh="shoulder_lift_link_collision"/>
                    <inertial                                   pos="0.1432 0.0072 -0.0001" mass="2.6615" 
                                                                fullinertia="0.0028 0.0111 0.0112 -0.0021 0 0"/>
                    <joint name="fetch/shoulder_lift"      class="fetch/shoulder_lift" pos="0 0 0"/>

