<mujocoinclude> 

    <!-- Fetch -->
    <body name="fetch/base_link" pos="0 0 0">
        <!-- === BASE LINK === === === === -->
        <!-- mobile body -->
        <inertial                                       pos="-0.0036 0.0 0.0014" mass="70.1294" 
                                                        fullinertia="1.225 1.2853 0.987 0.0099 0.0062 -0.0034" />
        <geom name="fetch/viz/base_link"                class="fetch/body/viz" mesh="base_link"/>

        <!-- Simplified the collision into boxes -->
        <geom name="fetch/col/base_link/box_body"       class="fetch/col" mesh="base_link_collision"/>

        <!-- world placement -->
        <joint name="fetch/world_x" armature="0.0001" axis="0 1 0" damping="1e+11" pos="0 0 0" type="slide"></joint>
        <joint name="fetch/world_y" armature="0.0001" axis="-1 0 0" damping="1e+11" pos="0 0 0" type="slide"></joint>
        <joint name="fetch/world_z" armature="0.0001" axis="0 0 1" damping="1e+0"  pos="0 0 0" type="slide"></joint>

        <!-- control summit base -->
        <joint name="fetch/pose/x"                      type="slide" pos="0 0 0.4" axis="0 1 0"  damping="15"/>
        <joint name="fetch/pose/y"                      type="slide" pos="0 0 0.4" axis="-1 0 0"  damping="15"/>
        <joint name="fetch/orie/z"                      type="hinge" pos="0 0 0.4" axis="0 0 1"  damping="10"/>

        <!-- === whl LINK === === === === -->
        <!-- Left Wheel -->
        <body name="fetch/whl/L_link" pos="0.0012914 0.18738 0.055325" quat="1 0 0 0">
            <geom name="fetch/viz/L_whl"                class="fetch/whl/viz" mesh="l_wheel_link"/>
            <geom name="fetch/col/L_whl"                class="fetch/col" mesh="l_wheel_link_collision"/>
            <inertial                                   pos="0 0 0" mass="4.3542" diaginertia="0.0045 0.005 0.0045"/>
            <joint name="fetch/L_whl"                   class="fetch/whl" pos="0 0 0"/>
